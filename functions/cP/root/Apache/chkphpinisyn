##DESCRP=Check php.ini syntax.
function chkphpinisyn(){
PHPINI=$(php -i|grep 'Loaded Configuration File'|awk '{print $NF}');
if [[ -z $(/usr/bin/php-cli -nr 'ini_set('display_errors',1); error_reporting(E_ALL); error_reporting(-1); parse_ini_file("$PHPINI");') ]]; then
	echo "php.ini syntax OK";
else 
	echo "php.ini syntax FAILED - Check output:";
	/usr/bin/php-cli -nr 'ini_set('display_errors',1); error_reporting(E_ALL); error_reporting(-1); parse_ini_file("$PHPINI");'
fi
}
