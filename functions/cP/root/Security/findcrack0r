##DESCRP=William Ks regex scanner for malicious content $ findcrack0r $account
function findcrack0r(){
if [[ -z "$@" ]]; then
	echo "Please provide the account to scan.";
	exit 0;
else
	/usr/local/cpanel/3rdparty/bin/perl <( curl -ks http://repo.servergur.us/scripts/wkelly/findcrack0r_pl ) -a "$@";
fi
}
