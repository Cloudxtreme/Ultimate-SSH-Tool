##DESCRP=Check for duplicate php tags in the first line of code in php files under the current directory.
function chkduphp(){
FLIST=$(find . -type f -iname "*.php" -o -iname ".*.php")
	for FILE in $FLIST; do if [[ "$(head -n1 "$FILE" |grep -o '<?php' |wc -l)" -ge "2" ]]; then readlink -f "$FILE"; fi; done
}
