##DESCRP=Pastebin function for KH customers!. # cat file |khpaste 
function khpaste() 
    {
        url=https://paste.privatesystems.net
        usage="$0 pastes into $server
        usage: $0 something
        example: '$0 pie' or 'ps aufx |$0'"
 
            if [ -z $1 ]; then
                    str=`cat /dev/stdin`;
            else
                    str=$1;
            fi
 
            if [ -z "$str" ]; then
                    echo $usage;
                    exit 1;
            fi
 
        output=`curl -s -X POST -d "$str" paste.privatesystems.net`
        echo $url/$output
}
